    <div class="requestForm">
        <form method="POST" id="requestForm" name="requestForm">
            <div class="input-group">
                <label for="requesterRole">Requester Role:</label>
                <input class="input--request" id="requesterRole" type="text" placeholder="requesterRole"
                    name="requesterRole">
            </div>
            <div class="input-group">

                <label for="resourceOwnerRole">Resource Owner Role:</label>
                <input class="input--request" id="resourceOwnerRole" type="text" placeholder="resourceOwnerRole"
                    name="resourceOwnerRole">
            </div>
            <div class="input-group">
                <label for="resourceType">Resource Type:</label>
                <input class="input--request" id="resourceType" type="text" placeholder="resourceType"
                    name="resourceType">
            </div>
            <div class="input-group">
                <label for="resourceId">Resource ID:</label>
                <input class="input--request" id="resourceId" type="text" placeholder="resourceId" name="resourceId">
            </div>
            <div class="input-group">
                <label for="idRequester">ID of Requester:</label>
                <input class="input--request" id="idRequester" type="text" placeholder="idRequester" name="idRequester">
            </div>
            <div class="input-group">
                <label for="idResourceOwner">ID of Resource Owner:</label>
                <input class="input--request" id="idResourceOwner" type="text" placeholder="idResourceOwner"
                    name="idResourceOwner">
            </div>
            <div class="p-t-10">
                <button class="btn btn--pill btn--green" type="submit" href="/addrequest"
                    style="margin-left: 300px;">Submit</button>
            </div>
        </form>
        <table>
            <tr>
                <th>Request ID</th>
                <th>Requester Role</th>
                <th>Resource Owner Role</th>
                <th>Resource Type</th>
                <th>Resource ID</th>
                <th>Requester ID</th>
                <th>Resource Owner ID</th>
                <th>Action</th>
            </tr>
            <% for(var i=0;i < data.length;i++){ %>
            <tr>
                <td id="requestId"><%= data[i].requestId%></td>  
                <td id="requesterRole"><%= data[i].requesterRole%></td>
                <td id="resourceOwnerRole"><%= data[i].resourceOwnerRole%></td>
                <td id="resourceType"><%= data[i].resourceType%></td>
                <td id="resourceId"><%= data[i].resourceId%></td>
                <td id="idRequester"><%= data[i].owner%></td>
                <td id="idResourceOwner"><%= data[i].resourceOwner%></td>
                <td>
                    <button class="request-button" type="accept" href="/accept/<%= data[i].requestId%>">Accept</button>
                    <button class="request-button" type="reject" href="/reject/<%= data[i].requestId%>">Reject</button>
                </td>
            </tr>
            <% } %>
        </table>
